<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" prefHeight="700.0" prefWidth="850.0" fx:controller="com.cc.corpapp.controller.DeliveryExecutivesController">
   <top>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
          <Button mnemonicParsing="false" onMouseClicked="#back" text="Back" />
        </buttons>
      </ButtonBar>
   </top>
   <center>
      <TableView fx:id="delivery_executives_list" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="id" prefWidth="75.0" text="id" />
          <TableColumn fx:id="email" prefWidth="124.0" text="email" />
        </columns>
      </TableView>
   </center>
</BorderPane>
